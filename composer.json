{
    "name": "arkounay/quick-admin-generator-bundle",
    "type": "symfony-bundle",
    "description": "QAG is a bundle that allows quick and simple administration backends generation for Symfony applications using Doctrine.",
    "keywords": [
        "symfony",
        "bundle",
        "admin",
        "generator",
        "crud"
    ],
    "homepage": "https://github.com/arkounay/QuickAdminGeneratorBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonin M",
            "email": "arkanen@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "twig/intl-extra": ">=3.1",
        "knplabs/knp-paginator-bundle": ">=5.3",
        "doctrine/annotations": ">=1.0",
        "doctrine/doctrine-bundle": ">=2.1",
        "symfony/form": ">=5.1.0",
        "symfony/framework-bundle": ">=5.1.0",
        "symfony/intl": ">=5.1.0",
        "symfony/property-info": ">=5.1.0",
        "symfony/yaml": ">=5.1.0",
        "symfony/twig-pack": "*",
        "sensio/framework-extra-bundle": ">=5.1",
        "symfony/orm-pack": "*",
        "symfony/expression-language": ">=5.1",
        "symfony/asset": ">=5.1",
        "symfony/security-bundle": ">=5.1",
        "symfony/validator": ">=5.1"
    },
    "autoload": {
        "psr-4": {
            "Arkounay\\Bundle\\QuickAdminGeneratorBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require-dev": {
        "symfony/test-pack": "^1.0"
    }
}
