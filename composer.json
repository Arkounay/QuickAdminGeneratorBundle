{
    "name": "arkounay/quick-admin-generator-bundle",
    "type": "symfony-bundle",
    "description": "QAG is a bundle that generates cruds admin for Symfony applications using Doctrine.",
    "keywords": [
        "symfony",
        "bundle",
        "admin",
        "generator",
        "crud"
    ],
    "homepage": "https://github.com/arkounay/QuickAdminGeneratorBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonin M",
            "email": "arkanen@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "knplabs/knp-paginator-bundle": ">=5.3",
        "doctrine/annotations": ">=1.0",
        "doctrine/doctrine-bundle": ">=2.1",
        "symfony/form": ">=5.1.0",
        "symfony/framework-bundle": ">=5.1.0",
        "symfony/intl": ">=5.1.0",
        "symfony/property-info": ">=5.1.0",
        "symfony/yaml": ">=5.1.0",
        "sensio/framework-extra-bundle": ">=5.1",
        "symfony/expression-language": ">=5.1",
        "symfony/asset": ">=5.1",
        "symfony/security-bundle": ">=5.1",
        "symfony/validator": ">=5.1",
        "arkounay/ux-collection": "^2.0",
        "symfony/twig-bundle": ">=5.1.0",
        "twig/intl-extra": "^3.3",
        "twig/extra-bundle": "^3.3",
        "twig/twig": "^2.12|^3.0",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.10",
        "symfony/proxy-manager-bridge": ">=5.1.0",
        "symfony/ux-turbo": "^2.0",
        "symfony/webpack-encore-bundle": "^1.14",
        "symfony/monolog-bundle": ">=3.1"
    },
    "autoload": {
        "psr-4": {
            "Arkounay\\Bundle\\QuickAdminGeneratorBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": ">=5.1.0",
        "symfony/css-selector": ">=5.1.0",
        "symfony/phpunit-bridge": ">=5.3"
    }
}
